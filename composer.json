{
    "name": "friendsofcake/cakephp-test-utilities",
    "description": "Package with support traits to ease unit testing.",
    "keywords": ["test", "unit testing", "unittesting", "phpunit"],
    "homepage": "https://github.com/friendsofcake/cakephp-test-utilities",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Winther",
            "role": "Author"
        }, {
            "name": "Andy Dawson",
            "role": "Author"
        }, {
            "name": "Frank de Graaf",
            "role": "Polisher"
        }
    ],
    "config": {
        "preferred-install": "source"
    },
    "autoload": {
        "psr-4": {
            "FriendsOfCake\\TestUtilities\\": "src",
            "FriendsOfCake\\TestUtilities\\Test\\": "tests"
        }
    },
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    }
}
